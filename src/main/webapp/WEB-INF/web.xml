<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>DeltaWeb</display-name>
    <description>Delta for Web.</description>

    <context-param>
        <param-name>PARAMETER_ENCODING</param-name>
        <param-value>UTF-8</param-value>
    </context-param>
    <!-- Define servlets that are included in the example application -->
    <servlet>
        <servlet-name>GeneaServlet</servlet-name>
        <servlet-class>delta.genea.web.GeneaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MusicServlet</servlet-name>
        <servlet-class>delta.music.web.MusicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UndergroundServlet</servlet-name>
        <servlet-class>delta.underground.web.UndergroundServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GeneaServlet</servlet-name>
        <url-pattern>/genea</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MusicServlet</servlet-name>
        <url-pattern>/music</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UndergroundServlet</servlet-name>
        <url-pattern>/underground</url-pattern>
    </servlet-mapping>

</web-app>
